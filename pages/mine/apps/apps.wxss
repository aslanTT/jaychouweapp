.apps-list {
  margin-top: 30rpx;
  width: 100%;
}

.apps-cell {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 140rpx;
  background-color: white;
}

.apps-info {
  height: 100%;
  display: flex;
  flex-direction: row;
  justify-content: left;
  align-items: center;
}

.apps-icon {
  width: 80rpx;
  height: 80rpx;
  margin-left: 20rpx;
  border-radius: 50rpx;
}

.apps-cell-title {
  margin-left: 32rpx;
  font-size: 32rpx;
  color: #666666;
}

.right-icon {
  margin-right: 20px;
  font-size: 32rpx;
  color: #666;
}

.apps-hover-cell {
  background-color: #999;
}